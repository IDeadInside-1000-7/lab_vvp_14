#include <stdio.h>
#include <locale.h>
int main()
{
	setlocale(LC_ALL, "Russian");
	int A, B;
	printf("Введите целые положительные числа A и B (A < B) через пробел: ");
	scanf("%i %i", &A, &B);
	if (A < B || A == B)
		printf("Пожалуйста, введите (A > B)!!!");
	else
	{
		printf("\nНа отрезке длины A размещено максимально возможное количество отрезков длины B (без наложений).");
		printf("\nПрограмма, не используя операции умножения и деления, найдет длину незанятой части отрезка A");
		int i, c = 0;
		for (i = A; i >= B; i -= B);
		printf("\n\nРезультат: %i\n", i);
	}
}
